.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page{
    width: 100%;
    height: 100vh;
    background-size: 100% 100%;
    background-image: url('https://i.loli.net/2021/02/01/k6yIE53JZ4PbRNQ.png'); 
}
button::after{ 
	border: none; 
}

button {
    margin-top: 20px;
    border-radius: 30px;
    background: linear-gradient(to right,#FFC0CB,#B0E0E6);
}
